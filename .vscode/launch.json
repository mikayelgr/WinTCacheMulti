            "name": "Debug unit tests in library 'win_tcache_multi'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--lib",
                    "--package=win_tcache_multi"
                ],
                "filter": {
                    "name": "win_tcache_multi",
                    "kind": "lib"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
